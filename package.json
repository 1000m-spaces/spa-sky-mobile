{
  "name": "neocafe",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "androidd": "react-native run-android",
    "ioss": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android": "node ./intro.js && npx react-native run-android",
    "ios": "node ./intro.js && npx react-native run-ios",
    "clean:android": "cd android &&./gradlew clean && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "codePush:android:prod": "appcenter codepush release-react -a ciaolinkdev/TeaThousand_Android -d Production",
    "codePush:android:dev": "appcenter codepush release-react -a ciaolinkdev/TeaThousand_Android -d Staging",
    "codePush:android:dev:force": "appcenter codepush release-react -a ciaolinkdev/NeoCafe_Android -d Develop --mandatory",
    "codePush:ios:prod": "appcenter codepush release-react -a ciaolinkdev/TeaThousand_ios -d Production",
    "codePush:ios:dev": "appcenter codepush release-react -a ciaolinkdev/TeaThousand_ios -d Staging",
    "build:dev:ios": "react-native run-ios --scheme dev --configuration dev",
    "build:prod:ios": "react-native run-ios --scheme neocafe --configuration Release",
    "build:dev:android": "react-native run-android --variant=dev --appId 'com.neocafe.development'",
    "build:prod:android": "react-native run-android --variant=release --appId 'com.neocafe'"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.6.3",
    "@react-native-async-storage/async-storage": "^1.18.2",
    "@react-native-community/checkbox": "^0.5.15",
